<window text="prova" size="640 480">
	<menu>
		<menuitem id="file_mnu" text="File">
			<menuitem id="open_mnu">Open</menuitem>
			<menuitem id="close_mnu">Close</menuitem>
		</menuitem>
		<menuitem id="edit_mnu" text="Edit"/>
	</menu>
	<vbox>
		<boxlayout expand="0">
			<toolbar>
				<toolbarbutton>bottone1</toolbarbutton>
				<toolbarcheckbutton>bottone2</toolbarcheckbutton>
				<toolbarseparator/>
				<toolbarradiogroup>
					<toolbarcheckbutton text="Radio1"/>
					<toolbarcheckbutton text="Radio2"/>
				</toolbarradiogroup>
			</toolbar>
			<hbox>
				<boxlayout expand="0"><label>Ciao, Io sono una label!</label></boxlayout>
				<boxlayout expand="1"><entry>ed io una entry</entry></boxlayout>
			</hbox>
		</boxlayout>
		<boxlayout expand="1">
			<hbox>
				<boxlayout expand="1">
					<list model="model1" stylemanager="style1">
						<listcolumn position="0" id="c1" name="Colonna 1"/>
						<listcolumn position="1" id="c2" name="Colonna 2"/>
					</list>
					<tree model="model1" stylemanager="style1" has-icons="1"></tree>
				</boxlayout>
			</hbox>
		</boxlayout>
		<boxlayout expand="0">
			<tab>
				<tabpage label="PageA">
					<combobox model="model1"></combobox>
				</tabpage>
				<tabpage label="PageB">
					<pluginlabel></pluginlabel>
				</tabpage>
			</tab>
		</boxlayout>
	</vbox>
</window>
